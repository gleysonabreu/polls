{
  "compilerOptions": {
    "outDir": "build",
    "target": "es2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDirs": [
      "src"
    ],
    "baseUrl": "src"
  },
  "include": [
    "src"
  ],
  "exclude": []
}
